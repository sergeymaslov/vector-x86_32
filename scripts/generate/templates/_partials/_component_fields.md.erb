<%- output_types = component.output_types -%>
<%- fields = component.fields -%>
<%- if output_types.include?("log") && fields.log -%>
<%= "#" * heading_depth %> <% if output_types.size > 1 %>Log <% end %>Fields

<%= fields_example(fields.log.fields_list, "log", root_key: fields.log.global_log_schema_key) %>

<%= fields(fields.log.fields_list, heading_depth: heading_depth + 1) %>

<%- end -%>
<%- if output_types.include?("metric") && fields.metric -%>
<%= "#" * heading_depth %> <% if output_types.size > 1 %>Metric <% end %>Fields

<%= fields_example(fields.metric.fields_list, "metric", root_key: fields.metric.global_log_schema_key) %>

<%= fields(fields.metric.fields_list, heading_depth: heading_depth + 1) %>

<%- end -%>
