<%- interfaces_style = interfaces.length > 4 -%>
<Tabs
  block={<%= !interfaces_style.to_json %>}
  centered={true}
  groupId="interfaces"
  placeholder="How did you install Vector?"
  select={<%= interfaces_style.to_json %>}
  size={<%= size.to_json %>}
  values={<%= interfaces.collect { |interface| {group: interface.type.humanize.pluralize, label: interface.title, value: interface.name } }.to_json %>}>
<%- interfaces.each do |interface| -%>
<TabItem value="<%= interface.name %>">

<%= interface_logs(interface) %>

</TabItem>
<%- end -%>
</Tabs>
